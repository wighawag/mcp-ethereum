{
	"name": "mcp-ethereum",
	"version": "0.0.1",
	"description": "MCP for ethereum",
	"author": "Ronan Sandford",
	"license": "MIT",
	"keywords": [
		"mcp",
		"ethereum"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/wighawag/mcp-ethereum.git"
	},
	"homepage": "https://github.com/wighawag/mcp-ethereum#readme",
	"bugs": {
		"url": "https://github.com/wighawag/mcp-ethereum/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"bin": {
		"mcp-ethereum": "dist/cli.js"
	},
	"files": [
		"dist",
		"src"
	],
	"sideEffects": false,
	"devDependencies": {
		"@types/node": "^25.2.0",
		"as-soon": "^0.1.5",
		"ldenv": "^0.3.16",
		"prettier": "^3.8.0",
		"tsx": "^4.21.0",
		"typescript": "^5.3.3",
		"vitest": "^4.0.18"
	},
	"scripts": {
		"prepublishOnly": "pnpm format:check && pnpm build",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"build": "tsc",
		"dev": "as-soon -w src pnpm build",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"dependencies": {
		"@modelcontextprotocol/sdk": "^1.25.3",
		"commander": "^14.0.3",
		"eip-1193": "^0.6.5",
		"remote-procedure-call": "^0.1.1",
		"viem": "^2.45.1",
		"zod": "^4.3.6"
	}
}
